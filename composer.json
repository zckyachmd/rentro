{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/react-starter-kit",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "bacon/bacon-qr-code": "^3.0",
        "barryvdh/laravel-dompdf": "^3.1",
        "godruoyi/php-snowflake": "^3.2",
        "inertiajs/inertia-laravel": "^2.0",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^12.0",
        "laravel/horizon": "^5.34",
        "laravel/reverb": "^1.6",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.10.1",
        "midtrans/midtrans-php": "^2.6",
        "paragonie/constant_time_encoding": "^2.6",
        "pragmarx/google2fa": "^8.0",
        "spatie/laravel-activitylog": "^4.10",
        "spatie/laravel-permission": "^6.21",
        "tightenco/ziggy": "^2.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "friendsofphp/php-cs-fixer": "^3.86",
        "larastan/larastan": "^3.0",
        "laravel/breeze": "^2.3",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.24",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-drift": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.2",
        "phpro/grumphp": "^2.14",
        "phpstan/phpstan": "^2.1",
        "squizlabs/php_codesniffer": "^3.13"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#34d399,#fb7185,#fdba74,#60a5fa\" \"php artisan serve\" \"php artisan queue:work --tries=1 --backoff=1 --sleep=1\" \"php artisan schedule:work\" \"php artisan pail --timeout=0\" \"npm run dev\" \"php artisan reverb:start\" --names=server,worker,schedule,logs,vite,reverb --kill-others"
        ],
        "dev:ssr": [
            "npm run build:ssr",
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#34d399,#fb7185,#fdba74,#60a5fa\" \"php artisan serve\" \"php artisan queue:work --tries=1 --backoff=1 --sleep=1\" \"php artisan schedule:work\" \"php artisan pail --timeout=0\" \"php artisan inertia:start-ssr\" \"php artisan reverb:start\" --names=server,worker,schedule,logs,ssr,reverb --kill-others"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ],
        "grumphp": "vendor/bin/grumphp run",
        "phpstan": "vendor/bin/phpstan analyse -c phpstan.neon",
        "phpstan:baseline": "vendor/bin/phpstan analyse -c phpstan.neon --generate-baseline"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "phpro/grumphp": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
